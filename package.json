{
  "name": "postget",
  "description": "Send requests and test your API's with Postget!",
  "copyright": "Copyright Â© 2020 ${author}",
  "productName": "Postget",
  "appId": "com.al5ina5.${name}",
  "version": "0.0.9",
  "main": "main/index.js",
  "scripts": {
    "clean": "rimraf dist renderer/.next renderer/out",
    "dev": "nodemon --watch main --ignore main/data/* --exec electron .",
    "start": "electron .",
    "build": "next build renderer && next export renderer",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "ace-builds": "^1.4.12",
    "axios": "^0.19.2",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "js-base64": "^3.2.4",
    "keypress": "^0.2.1",
    "lowdb": "^1.0.0",
    "mousetrap": "^1.6.5",
    "react-ace": "^9.1.1",
    "react-monaco-editor": "^0.39.1",
    "react-typist": "^2.0.5",
    "sass": "^1.26.10"
  },
  "author": "Sebastian Alsina <alsinas@me.com>",
  "license": "MIT",
  "repository": "https://github.com/al5ina5/postget.git"
}